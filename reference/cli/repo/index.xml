&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>userid: &lt;span style="color:#0000cf;font-weight:bold">1&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>org: github
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>name: octocat
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>fullname: github/octocat
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>link: https://github.com/github/octocat
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>clone: https://github.com/github/octocat.git
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>branch: main
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>buildlimit: &lt;span style="color:#0000cf;font-weight:bold">10&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>timeout: &lt;span style="color:#0000cf;font-weight:bold">60&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>timeout: &lt;span style="color:#0000cf;font-weight:bold">0&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>visibility: public
