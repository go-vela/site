&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>trusted: &lt;span style="color:#204a87">false&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>active: &lt;span style="color:#204a87">true&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>allowpull: &lt;span style="color:#204a87">true&lt;/span>
